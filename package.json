{
  "name": "flow-wing",
  "version": "1.0.3",
  "description": "A simple library to easily build complex flows through composed/piped tasks flows",
  "keywords": [
    "async",
    "compose",
    "concurrent",
    "context",
    "control",
    "flow",
    "parallel",
    "pipe",
    "runner",
    "series",
    "shared",
    "task",
    "waterfall"
  ],
  "homepage": "https://github.com/milsosa/flow-wing#readme",
  "bugs": {
    "url": "https://github.com/milsosa/flow-wing/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/milsosa/flow-wing.git"
  },
  "license": "MIT",
  "author": "Milton Sosa <sosamilton19@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && babel --copy-files --out-dir dist lib",
    "prepublishOnly": "npm run build",
    "test": "xo && nyc ava -v",
    "test:watch": "nyc ava -v --watch"
  },
  "xo": {
    "esnext": true,
    "overrides": [
      {
        "files": "examples/**/*.js",
        "rules": {
          "no-unused-vars": 1
        }
      }
    ],
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "capitalized-comments": "off",
      "no-use-extend-native/no-use-extend-native": "off"
    },
    "space": 2
  },
  "dependencies": {
    "debug": "^4.1.0",
    "is-promise": "^2.1.0",
    "lodash.defaults": "^4.2.0",
    "lodash.isfunction": "^3.0.9",
    "lodash.isplainobject": "^4.0.6",
    "lodash.map": "^4.6.0",
    "lodash.partial": "^4.2.1",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "ava": "^0.25.0",
    "axios": "^0.18.0",
    "nyc": "^13.1.0",
    "range": "^0.0.3",
    "sinon": "^7.1.1",
    "xo": "^0.23.0"
  },
  "engines": {
    "node": ">=4"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
